<?xml version="1.0" encoding="UTF-8"?>
<Context>
    <Resource name="jdbc/GincoPool"
        auth="Container"
        type="javax.sql.DataSource"
        driverClassName="org.postgresql.Driver"
        url="jdbc:postgresql://${ginco.postgres.host}:5432/${ginco.postgres.db}?useUnicode=true&amp;characterEncoding=UTF-8&amp;useFastDateParsing=false"
        username="${ginco.postgres.user}"
        password="${ginco.postgres.password}"
    />
</Context>
